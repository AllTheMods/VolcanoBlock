{
  // The template which will be used for spawn only
  // Example: 
  // {
  //     "name": "default",
  //     "desc": "",
  //     "file": "default.nbt",
  //     "spawns": "default",
  //     "direction": "south",
  //     "offset": [ 0, 0 ],
  //     "offsetY": 0,
  //     "surroundingMargin": 0,
  //     "surroundingBlocks": "default"
  // }
  "spawn": null,
  
  "spawns": {
    "default": [
      [ 6, 3, 5 ]
    ]
  },
  
  // A list of blocks which can be used to surround islands/caves.
